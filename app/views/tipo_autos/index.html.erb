<p id="notice"><%= notice %></p>

<h1>Tipo Autos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_autos.each do |tipo_auto| %>
      <tr>
        <td><%= tipo_auto.nome %></td>
        <td><%= link_to 'Show', tipo_auto %></td>
        <td><%= link_to 'Edit', edit_tipo_auto_path(tipo_auto) %></td>
        <td><%= link_to 'Destroy', tipo_auto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Auto', new_tipo_auto_path %>
